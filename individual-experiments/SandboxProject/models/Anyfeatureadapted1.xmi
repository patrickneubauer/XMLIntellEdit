      <anyGenericElem elemName="deliveryInformation" elemValue="delivery information">
        <anyGenericAttr
            attrName="orderType"
            attrValue="express"/>
        <anyGenericAttr
            attrName="deliveryCountry"
            attrValue="Austria"/>
        <childElem elemName="deliveringTo" elemValue="delivering to">
          <anyGenericAttr
              attrName="firstName"
              attrValue="Manuel"/>
          <anyGenericAttr
              attrName="lastName"
              attrValue="Wimmer"/>
        </childElem>
      </anyGenericElem>
    </properties>
  </rootElement>
</anyfeatureadapted:DocumentRoot>
