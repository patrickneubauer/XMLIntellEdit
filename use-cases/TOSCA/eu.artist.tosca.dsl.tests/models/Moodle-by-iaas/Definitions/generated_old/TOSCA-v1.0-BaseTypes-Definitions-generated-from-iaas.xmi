      <RequirementDefinition lowerBound="0" name="volumes" requirementType="tns:VolumeAttachmentRequirement" upperBound="unbounded"/>
    </RequirementDefinitions>
    <CapabilityDefinitions>
      <CapabilityDefinition capabilityType="tns:OperatingSystemContainerCapability" lowerBound="0" name="os" upperBound="1"/>
